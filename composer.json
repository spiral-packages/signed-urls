{
  "name": "spiral/signed-urls",
  "description": "Create and validate signed URLs in Spiral Framework",
  "keywords": [
    "spiral",
    "spiral-framework",
    "signed-urls",
    "php",
    "php81",
    "url-generator"
  ],
  "homepage": "https://github.com/spiral/signed-urls",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Buchnev",
      "email": "butschster@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "spiral/boot": "^3.0",
    "spiral/router": "^3.0"
  },
  "require-dev": {
    "spiral/framework": "^3.0",
    "spiral/testing": "^2.0",
    "vimeo/psalm": "^4.9"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\SignedUrls\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\SignedUrls\\Tests\\App\\": "tests/app",
      "Spiral\\SignedUrls\\Tests\\": "tests/src"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm --config=psalm.xml ./src"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "spiral/composer-publish-plugin": true
    }
  },
  "extra": {
    "spiral": {
      "bootloaders": [
        "Spiral\\SignedUrls\\Bootloader\\SignedUrlsBootloader"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
